<h1>Welcome, <%= @user.email %>!</h1>

<%= link_to "Create an Event", new_event_path %>

<h2>Created events</h2>
<ul>
  <% @created_events.each do |event| %>
    <li>
      <%= link_to event.name, event_path(event) %>
    </li>
  <% end %>
</ul>

<h2>Attended events</h2>
<ul>
  <% @attended_events.each do |event| %>
    <li>
      <%= link_to event.name, event_path(event) %>
    </li>
  <% end %>
</ul>

